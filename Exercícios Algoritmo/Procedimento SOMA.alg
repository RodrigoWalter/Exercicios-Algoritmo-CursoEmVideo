algoritmo "Procedimento Soma"
// Função : Realizar a soma de dois números
// Autor : Rodrigo Walter
// Data : 20/07/2022
var
   N1,N2:inteiro
Procedimento Soma(A,B:inteiro)
Inicio
    EscrevaL("Recebi o valor:", A)
    EscrevaL("Recebi o valor:", B)
    EscrevaL("A soma dos dois valores é:",A+B)
FimProcedimento
inicio
    N1 <- 3
    N2 <- 5
    Soma(N1,N2)
fimalgoritmo
